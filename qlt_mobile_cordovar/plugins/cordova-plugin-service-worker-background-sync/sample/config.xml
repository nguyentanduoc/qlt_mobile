<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.backgroundsyncdemo" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="serviceworker" value="sw.js"></preference>
    <preference name="minperiod" value="1500"></preference>
    <preference name="syncpushback" value="3000"></preference>
